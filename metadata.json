{
  "name": "katello-certs",
  "version": "15.0.0",
  "author": "katello",
  "summary": "Deploys CA and required certs for a Foreman and Katello installation.",
  "license": "GPL-3.0+",
  "source": "https://github.com/theforeman/puppet-certs.git",
  "project_page": "https://github.com/theforeman/puppet-certs",
  "issues_url": "https://projects.theforeman.org/projects/katello/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.25.0 < 9.0.0"
    },
    {
      "name": "puppet-extlib",
      "version_requirement": ">= 3.0.0 < 7.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 1.0.0 < 8.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.15.0 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
  ]
}
